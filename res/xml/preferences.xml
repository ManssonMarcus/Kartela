<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
	
	<PreferenceCategory 
	    android:title="@string/settings_header"> 
	</PreferenceCategory>
	
	<MultiSelectListPreference
	    android:dialogTitle="@string/pref_sync"
	    android:key="pref_sync"
	    android:summary="Välj dagar"
	    android:title="@string/pref_sync"
	    android:entries="@array/pref_syncConnectionTypes_days"
	    android:entryValues="@array/pref_syncConnectionTypes_values"
	    android:defaultValue="@array/pref_syncConnectionTypes_values"
	    />
    	
	 <com.example.kartela.TimePreference 
	     android:key="settings_time" 
	     android:title="@string/pref_timeTitle"/>
	 
	 	<Preference
		    android:title="Påminnelsenotifikation"
		    android:summary="Ställ in datum och tid">

		    <intent 
		        android:action="android.intent.action.VIEW"
		        android:targetPackage="com.example.kartela"
		        android:targetClass="com.example.kartela.NotificationActivity"
		    />

		</Preference>
        
	<PreferenceCategory android:title="@string/pref_send">
	     <EditTextPreference
    	     android:key="pref_mail"
		     android:summary="exempel@mail.se"
		     android:title="Mejladress">
		 </EditTextPreference>    
	</PreferenceCategory>
	
</PreferenceScreen>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context=".TestDatabaseActivity" >
    
    <TextView
	    android:id="@+id/progress_combitech"
	    android:background="@drawable/combitech_rect"
	    android:layout_height="50dp"
	    android:layout_width="500dp" />
    
    <TextView
	    android:id="@+id/progress_saab"
	    android:background="@drawable/saab_rect"
	    android:layout_height="50dp"
	    android:layout_width="500dp" />
    
    <TextView
	    android:id="@+id/progress_volvo"
	    android:background="@drawable/volvo_rect"
	    android:layout_height="50dp"
	    android:layout_width="500dp" />
    
    <TextView
	    android:id="@+id/progress_helikopter"
	    android:background="@drawable/helikopter_rect"
	    android:layout_height="50dp"
	    android:layout_width="500dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/send_mail_button"
        android:onClick="sendTimeReportMail" />

    <Button    
   		android:id="@+id/deleteall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="delete" />

    
    <ListView
        android:id="@android:id/list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFF"
        android:text="@+string/hello" />

</LinearLayout>
